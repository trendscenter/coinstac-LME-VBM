{
  "meta": {
    "name": "decentralized linear mixed effects model - VBM",
    "id": "dlme-vbm",
    "version": "v1.0.0",
    "repository": "https:\/\/github.com\/trendscenter\/coinstac-LME-VBM",
    "description": "decentralized linear mixed effects model for vbm data"
  },
  "computation":
  {
    "type": "docker",
    "dockerImage": "dlme-vbm",
    "command": ["python","\/computation\/scripts\/local.py"],
    "remote":
    {
      "type": "docker",
      "dockerImage": "dlme-vbm",
      "command": ["python","\/computation\/scripts\/remote.py"]
    },
    "input":
    {
      "fixed_covariates":
      {
        "label":"fixed_covariates",
        "type":"string",
        "items":["fixed_effects"],
        "extensions":["csv"]
      },
      "random_covariates":
      {
        "label":"random_covariates",
        "type":"string",
        "items":["random_effects"],
        "extensions":["csv"]
      },
      "dependents":
      {
        "label":"dependents",
        "type":"string",
        "items":["VBM"],
        "extensions":[["csv","txt"]]
      },
      "mask_threshold":
      {
        "label":"mask_threshold",
        "type":"number"
      },
      "voxel_size":
      {
        "label":"voxel_size",
        "type":"number"
      },
      "contrasts":
      {
        "label":"contrasts",
        "type": "array",
        "items":
        {
          "name": {"label":"name","type":"string"},
          "vector": {"label":"vector","type":"array"}
        }
      }
    },
    "output":
    {
      "global_result_images":
      {
        "label": "global_result_images",
        "type": "array"
      },
      "local_result_images":
      {
        "label": "global_result_images",
        "type": "array"
      }
    }
  }
}
